{% extends "layout.njk" %}

{% block main %}

use spl_program_error::*;

#[spl_program_error]
pub enum {{ program.name | pascalCase }}Error {
{% for error in errors | sort(false, false, 'code') %}
    /// 0x{{ error.code.toString(16) | upper }} - {{ error.message }}
    #[error("{{ error.message }}")]
    {{ error.name | pascalCase }},
{% endfor %}
}

{% endblock %}
